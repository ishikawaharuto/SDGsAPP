{
  "manifest_version": 3,
  "name": "Digital Carbon Tracer",
  "version": "1.0",
  "description": "ブラウジング活動を自動で記録し、カーボンフットプリントを計算します。",
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
